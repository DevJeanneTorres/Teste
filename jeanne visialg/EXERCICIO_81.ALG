Algoritmo "exercicio_81"
// Jeanne Torres da Silva
Var
   contador,num : inteiro

Inicio
   contador <- 1
   repita
      se (contador mod 2 = 0)entao
         num <- num+1
      fimse
      contador <- contador +1
   ate contador > 20
   escreva ("a quantidade de numeros pares sao :",num)


Fimalgoritmo